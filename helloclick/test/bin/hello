#!/Users/benhubsch/Dropbox/SideProjects/helloclick/test/bin/python3.6
# EASY-INSTALL-ENTRY-SCRIPT: 'HellowWorld','console_scripts','hello'
__requires__ = 'HellowWorld'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('HellowWorld', 'console_scripts', 'hello')()
    )
